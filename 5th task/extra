#include <fstream> 
#include <iostream>
#include <cstring>

using namespace std;

int main()
{
	int a,b,N;
	ifstream fin("input.txt");
	fin >> a;
 	fin >> b;
 	fin >> N;
 	int mass[N];
 	for(int i=0;i<N;++i)
 	{
 		fin >> mass[i];
	}
	string msg;
	fin >> msg;
 	cout << "a = " << a << endl;
 	cout << "b = " << b << endl;
 	cout << "N = " << N << endl;
 	cout << "mass = ";
 	for(int i=0;i<N;++i)
 	{
 		cout << mass[i] << " ";
	}
	cout << endl;
 	cout << "msg = " << msg << endl;
 	fin.close();
	return 0;
}
